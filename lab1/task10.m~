Fs = 8000; dT = 0.5;
n = 0:1/Fs:4;
x = zeros(size(n));
for i = 1:length(x)
    x(i) = sound(n(i));
end
plot(n,x)
player = audioplayer(x, Fs);
play(player);
%audiowrite('10.wav', f10, Fs);


function y = sound(n)
    switch fix(n / (0.5))
        case 0 
            y = 659;
        case 1 
            y = 622;
        case 2 
            y = 659;
        case 3 
            y = 622;
        case 4 
            y = 494;
        case 5 
            y = 587;
        case 6 
            y = 523;
        case 7 
            y = 440;
        otherwise
            y = 440;
    end
    y = sin(2 * pi * y * n)
end


function [y] = z10_2(n, f)
    for i = 1:length(n)
        y(i) = ;
    end
end